@using g212102053_mehmet_kaan_akcan_odev.Models.Siniflar
@model List<Contact>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<br />
<section id="contact" class="contact">
    <div class="container">
        @foreach (var h in Model) { 
    <div>
        <iframe style="border:0; width: 100%; height: 270px;"  src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3022.052674515055!2d29.932952515406477!3d40.760866179326634!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14cb457c585cbb81%3A0x19699efc43fa706f!2zS29jYWVsaXNwb3IgS3Vsw7xiw7w!5e0!3m2!1str!2str!4v1684884693175!5m2!1str!2str" frameborder="0" allowfullscreen></iframe>
    </div>

        <div class="row mt-5">

            <div class="col-lg-4">
                <div class="info">
                    <div class="address">
                        <i class="bi bi-geo-alt"></i>
                        <h4>Lokasyon:</h4>
                        <p>@h.Location</p>
                    </div>

                    <div class="email">
                        <i class="bi bi-envelope"></i>
                        <h4>Email:</h4>
                        <p>@h.Email</p>
                    </div>

                    <div class="phone">
                        <i class="bi bi-phone"></i>
                        <h4>Telefon:</h4>
                        <p>@h.Call</p>
                    </div>

                </div>

            </div>


        </div>
        }
    </div>
</section><!-- End Contact Section -->
